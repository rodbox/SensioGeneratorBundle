{% block phpdoc_method_annotations %}
{% if 'annotation' == format %}
     * @Route("/{{ route_name_prefix }}_print/{id}", name="{{ route_name_prefix }}_print")
{% endif %}
{% endblock phpdoc_method_annotations %}
     */
{% block method_definition %}
    public function printAction({{ entity_class }} ${{ entity_singularized }})
